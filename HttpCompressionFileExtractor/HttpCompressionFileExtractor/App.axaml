<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:sty="using:FluentAvalonia.Styling"
			 xmlns:local="clr-namespace:HttpCompressionFileExtractor"
			 xmlns:api="using:Eruru.CSharp.Api.FluentAvalonia"
             x:Class="HttpCompressionFileExtractor.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
	<Application.Resources>
		<api:FileSizeTextConverter x:Key="FileSizeTextConverter"/>
		<api:DateTimeLocalTextConverter x:Key="DateTimeLocalTextConverter"/>
	</Application.Resources>
    <Application.Styles>
        <sty:FluentAvaloniaTheme/>
    </Application.Styles>
</Application>
